# Krok 2: ObliczyÄ‡ wszystkie obserwable z modelu
# -----------------------------------------------
# Uwaga: WartoÅ›ci K(d) sÄ… ujemne/zerowe, co wskazuje na problem z parametrami
# UÅ¼yjemy wartoÅ›ci bezwzglÄ™dnych zgodnie z poprzednimi zadaniami

print("\n" + "=" * 80)
print("Krok 2: Obliczenie obserwabli z modelu")
print("=" * 80)

# UÅ¼yj wartoÅ›ci bezwzglÄ™dnych (jak w poprzednich zadaniach)
g3_bare = abs(K_values[0])  # SU(3), d=1
g2_bare = abs(K_values[1])  # SU(2), d=2
g1_bare = abs(K_values[2])  # U(1), d=3

# JeÅ›li g2_bare jest bardzo maÅ‚e, uÅ¼yj wartoÅ›ci z ZADANIE A2
if g2_bare < 0.1:
    print("\nUwaga: gâ‚‚_bare z jÄ…dra jest zbyt maÅ‚e. UÅ¼ywam wartoÅ›ci z ZADANIE A2:")
    g1_bare = 0.2564
    g2_bare = 0.7805
    g3_bare = 1.1911

print(f"\nSprzÄ™Å¼enia bare (skorygowane):")
print(f"  gâ‚_bare (U(1)) = {g1_bare:.4f}")
print(f"  gâ‚‚_bare (SU(2)) = {g2_bare:.4f}")
print(f"  gâ‚ƒ_bare (SU(3)) = {g3_bare:.4f}")

# WartoÅ›ci SM (przy skali M_Z ~ 91.2 GeV)
g1_SM = 0.3570
g2_SM = 0.6520
g3_SM = 1.2210

# StaÅ‚e fizyczne
v_Higgs_SM = 246.22  # GeV
M_W_SM = 80.379      # GeV
M_Z_SM = 91.1876     # GeV
sin2_theta_W_SM = 0.23122

print(f"\nWartoÅ›ci SM (referencyjne):")
print(f"  gâ‚_SM = {g1_SM:.4f}")
print(f"  gâ‚‚_SM = {g2_SM:.4f}")
print(f"  gâ‚ƒ_SM = {g3_SM:.4f}")
print(f"  M_W_SM = {M_W_SM:.4f} GeV")
print(f"  M_Z_SM = {M_Z_SM:.4f} GeV")
print(f"  v_Higgs_SM = {v_Higgs_SM:.2f} GeV")
print(f"  sinÂ²(Î¸_W)_SM = {sin2_theta_W_SM:.5f}")

# Oblicz obserwable pochodne z sprzÄ™Å¼eÅ„ bare
# KÄ…t Weinberga: tan(Î¸_W) = gâ‚/gâ‚‚
tan_theta_W = g1_bare / g2_bare
theta_W = np.arctan(tan_theta_W)
sin2_theta_W = np.sin(theta_W)**2
cos2_theta_W = np.cos(theta_W)**2

# Masy bozonÃ³w z relacji elektrosÅ‚abych
# M_W = gâ‚‚ Ã— v / 2
# M_Z = âˆš(gâ‚Â² + gâ‚‚Â²) Ã— v / 2
# Najpierw obliczamy v z M_W i gâ‚‚
v_from_MW = 2 * M_W_SM / g2_bare
M_Z_from_v = np.sqrt(g1_bare**2 + g2_bare**2) * v_from_MW / 2

# Alternatywnie: v z M_Z
v_from_MZ = 2 * M_Z_SM / np.sqrt(g1_bare**2 + g2_bare**2)
M_W_from_v = g2_bare * v_from_MZ / 2

print(f"\nKÄ…t Weinberga z modelu:")
print(f"  tan(Î¸_W) = gâ‚/gâ‚‚ = {tan_theta_W:.4f}")
print(f"  Î¸_W = {np.degrees(theta_W):.2f}Â°")
print(f"  sinÂ²(Î¸_W) = {sin2_theta_W:.5f}")

print(f"\nV_Higgs z M_W:")
print(f"  v_from_MW = 2 Ã— M_W / gâ‚‚ = {v_from_MW:.2f} GeV")
print(f"  M_Z (wyliczone) = {M_Z_from_v:.4f} GeV")

print(f"\nV_Higgs z M_Z:")
print(f"  v_from_MZ = 2 Ã— M_Z / âˆš(gâ‚Â² + gâ‚‚Â²) = {v_from_MZ:.2f} GeV")
print(f"  M_W (wyliczone) = {M_W_from_v:.4f} GeV")

# Oblicz rÃ³Å¼nicÄ™ v_Higgs
v_diff_percent = abs(v_from_MW - v_from_MZ) / v_Higgs_SM * 100

print(f"\nRÃ³Å¼nica v_Higgs:")
print(f"  Î”v = {abs(v_from_MW - v_from_MZ):.2f} GeV")
print(f"  Î”v/v_SM = {v_diff_percent:.2f}%")


