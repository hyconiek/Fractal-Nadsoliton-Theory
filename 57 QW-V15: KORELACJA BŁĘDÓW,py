# Krok 4: ObliczyÄ‡ macierz korelacji bÅ‚Ä™dÃ³w
# ------------------------------------------

print("\n" + "=" * 80)
print("Krok 4: Macierz korelacji bÅ‚Ä™dÃ³w")
print("=" * 80)

# Przygotuj dane do analizy korelacji
# Potrzebujemy zebraÄ‡ bÅ‚Ä™dy w formie, ktÃ³ra pozwala na obliczenie korelacji
# PoniewaÅ¼ mamy pojedynczy punkt (jeden zestaw parametrÃ³w), uÅ¼yjemy analizy
# zaleÅ¼noÅ›ci funkcjonalnych miÄ™dzy bÅ‚Ä™dami

# Najpierw utworzymy rozszerzonÄ… analizÄ™ przez perturbacje parametrÃ³w
# To pozwoli nam zobaczyÄ‡ jak bÅ‚Ä™dy korelujÄ… przy zmianach parametrÃ³w

print("\nUwaga: Standardowa analiza korelacji wymaga wielu punktÃ³w danych.")
print("Przeprowadzamy analizÄ™ perturbacyjnÄ… wokÃ³Å‚ parametrÃ³w zunifikowanych.")
print("Generujemy prÃ³bki przez maÅ‚e perturbacje Î±_geo i Î²_tors.")

# Wygeneruj prÃ³bki przez perturbacje parametrÃ³w
n_samples = 100
np.random.seed(42)

# Perturbacje: Â±10% wokÃ³Å‚ wartoÅ›ci centralnych
alpha_geo_samples = alpha_geo * (1 + 0.1 * np.random.randn(n_samples))
beta_tors_samples = beta_tors * (1 + 0.1 * np.random.randn(n_samples))

# Oblicz bÅ‚Ä™dy dla kaÅ¼dej prÃ³bki
errors_matrix = []

for i in range(n_samples):
    alpha_g = alpha_geo_samples[i]
    beta_t = beta_tors_samples[i]

    # Oblicz K(d) dla tej prÃ³bki
    K_vals = np.array([coupling_kernel(d, alpha_g, beta_t, omega, phi) for d in d_values])

    # JeÅ›li K(2) jest zbyt maÅ‚e, uÅ¼yj proporcjonalnej skali z wartoÅ›ci referencyjnych
    if abs(K_vals[1]) < 0.1:
        scale = abs(K_vals[0]) / 1.1911  # Skala wzglÄ™dem g3
        g3_temp = abs(K_vals[0])
        g2_temp = 0.7805 * scale
        g1_temp = 0.2564 * scale
    else:
        g1_temp = abs(K_vals[2])
        g2_temp = abs(K_vals[1])
        g3_temp = abs(K_vals[0])

    # Oblicz obserwable
    tan_theta = g1_temp / g2_temp
    theta = np.arctan(tan_theta)
    sin2_theta = np.sin(theta)**2

    v_MW = 2 * M_W_SM / g2_temp
    M_Z_calc = np.sqrt(g1_temp**2 + g2_temp**2) * v_MW / 2

    v_MZ = 2 * M_Z_SM / np.sqrt(g1_temp**2 + g2_temp**2)
    M_W_calc = g2_temp * v_MZ / 2

    # Oblicz bÅ‚Ä™dy dla tej prÃ³bki
    sample_errors = {
        'g1': (g1_temp - g1_SM) / g1_SM,
        'g2': (g2_temp - g2_SM) / g2_SM,
        'g3': (g3_temp - g3_SM) / g3_SM,
        'g2/g1': (g2_temp/g1_temp - g2_SM/g1_SM) / (g2_SM/g1_SM),
        'g3/g2': (g3_temp/g2_temp - g3_SM/g2_SM) / (g3_SM/g2_SM),
        'g3/g1': (g3_temp/g1_temp - g3_SM/g1_SM) / (g3_SM/g1_SM),
        'sin2_theta_W': (sin2_theta - sin2_theta_W_SM) / sin2_theta_W_SM,
        'theta_W_deg': (np.degrees(theta) - np.degrees(np.arcsin(np.sqrt(sin2_theta_W_SM)))) / np.degrees(np.arcsin(np.sqrt(sin2_theta_W_SM))),
        'M_Z_from_MW': (M_Z_calc - M_Z_SM) / M_Z_SM,
        'M_W_from_MZ': (M_W_calc - M_W_SM) / M_W_SM,
        'v_from_MW': (v_MW - v_Higgs_SM) / v_Higgs_SM,
        'v_from_MZ': (v_MZ - v_Higgs_SM) / v_Higgs_SM,
    }

    errors_matrix.append(list(sample_errors.values()))

# StwÃ³rz DataFrame z bÅ‚Ä™dami
error_names = list(sample_errors.keys())
df_errors = pd.DataFrame(errors_matrix, columns=error_names)

print(f"\nWygenerowano {n_samples} prÃ³bek przez perturbacje parametrÃ³w zunifikowanych")
print(f"Zakres perturbacji: Â±10% wokÃ³Å‚ Î±_geo={alpha_geo:.4f}, Î²_tors={beta_tors:.4f}")

# Oblicz macierz korelacji
correlation_matrix = df_errors.corr()

print("\n" + "=" * 80)
print("MACIERZ KORELACJI BÅÄ˜DÃ“W")
print("=" * 80)
print("\nWspÃ³Å‚czynnik korelacji Pearsona Ï âˆˆ [-1, 1]:")
print("  |Ï| > 0.8: SILNA korelacja")
print("  0.5 < |Ï| â‰¤ 0.8: UMIARKOWANA korelacja")
print("  |Ï| â‰¤ 0.5: SÅABA korelacja")
print("\n")


