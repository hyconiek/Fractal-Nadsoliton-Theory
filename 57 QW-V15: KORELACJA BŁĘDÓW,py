# Krok 5: Identyfikacja silnych korelacji
# ----------------------------------------

print("\n" + "=" * 80)
print("Krok 5: Identyfikacja silnych korelacji (|Ï| > 0.8)")
print("=" * 80)

# ZnajdÅº wszystkie pary z silnymi korelacjami
strong_correlations = []
n_obs = len(error_names)

for i in range(n_obs):
    for j in range(i+1, n_obs):  # Tylko gÃ³rny trÃ³jkÄ…t (bez powtÃ³rzeÅ„)
        rho = correlation_matrix.iloc[i, j]
        if abs(rho) > 0.8:
            strong_correlations.append({
                'Observable 1': error_names[i],
                'Observable 2': error_names[j],
                'Ï': rho,
                '|Ï|': abs(rho),
                'Type': 'Dodatnia' if rho > 0 else 'Ujemna'
            })

df_strong = pd.DataFrame(strong_correlations).sort_values('|Ï|', ascending=False)

if len(df_strong) > 0:
    print(f"\nZnaleziono {len(df_strong)} silnych korelacji (|Ï| > 0.8):")
    print("-" * 100)
    print(df_strong.to_string(index=False))
    print("-" * 100)
else:
    print("\nBrak silnych korelacji (|Ï| > 0.8).")
    print("Szukam umiarkowanych korelacji (0.5 < |Ï| â‰¤ 0.8)...")

# JeÅ›li brak silnych, znajdÅº umiarkowane
if len(df_strong) == 0:
    moderate_correlations = []
    for i in range(n_obs):
        for j in range(i+1, n_obs):
            rho = correlation_matrix.iloc[i, j]
            if 0.5 < abs(rho) <= 0.8:
                moderate_correlations.append({
                    'Observable 1': error_names[i],
                    'Observable 2': error_names[j],
                    'Ï': rho,
                    '|Ï|': abs(rho),
                    'Type': 'Dodatnia' if rho > 0 else 'Ujemna'
                })

    df_moderate = pd.DataFrame(moderate_correlations).sort_values('|Ï|', ascending=False)

    if len(df_moderate) > 0:
        print(f"\nZnaleziono {len(df_moderate)} umiarkowanych korelacji (0.5 < |Ï| â‰¤ 0.8):")
        print("-" * 100)
        print(df_moderate.to_string(index=False))
        print("-" * 100)
    else:
        print("\nBrak rÃ³wnieÅ¼ umiarkowanych korelacji.")
        print("Wszystkie korelacje sÄ… sÅ‚abe (|Ï| â‰¤ 0.5).")

